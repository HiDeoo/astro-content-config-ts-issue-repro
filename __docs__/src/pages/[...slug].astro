---
import { getCollection } from 'astro:content';

export async function getStaticPaths() {
  const docs = await getCollection('docs');
  return docs.map((doc) => ({
    params: { slug: doc.id === 'index' ? '' : doc.id },
    props: { doc },
  }));
}

const { doc } = Astro.props;
---

{doc.data.title}
